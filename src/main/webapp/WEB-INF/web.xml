<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
<servlet>
<servlet-name>GetProductController</servlet-name>
<servlet-class>com.example.priticalcswclient.controller.GetProductController</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>GetProductController</servlet-name>
<url-pattern>/products</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>CreateProductController</servlet-name>
<servlet-class>com.example.priticalcswclient.controller.CreateProductController</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>CreateProductController</servlet-name>
<url-pattern>/products/create</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>UpdateProductController</servlet-name>
<servlet-class>com.example.priticalcswclient.controller.UpdateProductController</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>UpdateProductController</servlet-name>
<url-pattern>/products/update</url-pattern>
</servlet-mapping>
        </web-app>
